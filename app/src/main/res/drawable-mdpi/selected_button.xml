<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#94C83F" />
      <stroke
          android:width="1dp"
          android:color="#b9c65f" />
     
      <padding
          android:left="3dp"
          android:top="1dp"
          android:right="3dp"
          android:bottom="1dp" />
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#94C83F"
          android:endColor="#94C83F"
          android:angle="270" />
      <stroke
          android:width="1dp"
          android:color="#b9c65f" />
     
      <padding
          android:left="3dp"
          android:top="1dp"
          android:right="3dp"
          android:bottom="1dp" />
    </shape>
  </item>
</selector>
