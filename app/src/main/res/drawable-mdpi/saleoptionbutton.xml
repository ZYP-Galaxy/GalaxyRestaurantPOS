<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" > 
       <shape>
      <gradient
          android:startColor="#94C83F"
          android:endColor="#94C83F"
          android:angle="270" />
      <stroke
          android:width="2dp"
          android:color="#FFFFFF" />    
      <padding
          android:left="3dp"
          android:top="10dp"
          android:right="3dp"
          android:bottom="1dp" />
    </shape>           
  </item>
  <item >   
     <shape>
      <gradient
          android:startColor="#FFFFFF"
          android:endColor="#E4E4E4"
          android:angle="270" />
      <stroke
          android:width="2dp"
          android:color="#FFFFFF" />     
      <padding
          android:left="3dp"
          android:top="10dp"
          android:right="3dp"
          android:bottom="1dp" />
    </shape>     
  </item>
</selector>
